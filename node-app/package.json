{
  "name": "demo-ts-node",
  "version": "1.0.0",
  "description": "demo typescript working with node express",
  "main": "app.js",
  "scripts": {
    "start": "tsc",
    "serve": "ts-node-dev --respawn --transpileOnly ./src/app.ts",
    "dev": "concurrently -k \"npm run serve\" \"npm run start-open --prefix ../ng-app\" ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "rimraf webpack-dist/server.bundle.js && webpack",
    "deploy": "tsc && npm run webpack && npm --prefix ../ng-app run build:prod && npm --prefix ../webpack-dist install",
    "deploy:server-only": "tsc && npm run webpack",
    "deploy:clientApp-only": "npm --prefix ../ng-app run build:prod",
    "deploy:server-with-packages": "tsc && npm run webpack"
  },
  "author": "Jacob Tan",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mongoose": "^5.5.28",
    "@types/multer": "^1.3.10",
    "@types/node-schedule": "^1.2.4",
    "@types/nodemailer": "^6.2.2",
    "@types/qrcode": "^1.3.4",
    "@types/shortid": "0.0.29",
    "@types/uuid": "^3.4.6",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.2",
    "copy-webpack-plugin": "^5.1.1",
    "nodemon": "^1.19.4",
    "ts-node-dev": "^1.0.0-pre.43",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.16.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^2.2.1",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.3.1",
    "promise": "^8.0.3",
    "qrcode": "^1.4.2",
    "rxjs": "^6.5.3",
    "shortid": "^2.2.15",
    "uuid": "^3.3.3"
  }
}
